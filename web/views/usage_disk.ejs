          <a href="#" class="btn btn-primary" id="menu-toggle">Menu</a>
          <!-- top  -->
          <% include ./top.ejs %>
          <p>Disk</p>
          <!--  -->
        </div>
        <!-- middle -->
        <!--  -->
      </div>

      <div id="chart_div" style='width: 900px; height: 500px;'></div>

      <!-- bottom -->
      <!--  -->
    </div>
  </div>

</div>
<script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
