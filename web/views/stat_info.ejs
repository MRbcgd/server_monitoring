  result.kernel = 'i dnt knw.';
  result.hostname = os.hostname();
  result.ip = ip.address();-->
      <div class="container">
      </div>
      <table class="table table-fixed">
        <thead>
          <tr>
            <th class="col-xs-3">항목</th>
            <th class="col-xs-3">내용</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td class="col-xs-3">CPU Model</td>
            <td class="col-xs-3"><%= data.model_name %></td>
          </tr>

          <tr>
            <td class="col-xs-3">Frequency</td>
            <td class="col-xs-3"><%= data.cpu_MHz %></td>
          </tr>
          <tr>
            <td class="col-xs-3">Cache size</td>
            <td class="col-xs-3"><%= data.cache_size %></td>
          </tr>
          <tr>
            <td class="col-xs-3">stepping</td>
            <td class="col-xs-3"><%= data.stepping %></td>
          </tr>
          <tr>
            <td class="col-xs-3">Vendor ID</td>
            <td class="col-xs-3"><%= data.vendor_id%></td>
          </tr>
          <tr>
            <td class="col-xs-3">Bogomips</td>
            <td class="col-xs-3"><%= data.bogomips %></td>
          </tr>
          <tr>
            <td class="col-xs-3">Memory (total)</td>
            <td class="col-xs-3"><%= data.MemTotal %></td>
          </tr>
          <tr>
            <td class="col-xs-3">Memory (free)</td>
            <td class="col-xs-3"><%= data.MemFree%></td>
          </tr>
          <tr>
            <td class="col-xs-3">Memory (swap)</td>
            <td class="col-xs-3"><%= data.MemSwap %></td>
          </tr>
          <tr>
            <td class="col-xs-3">OS</td>
            <td class="col-xs-3"><%= data.platform %></td>
          </tr>
          <tr>
            <td class="col-xs-3">Kernel</td>
            <td class="col-xs-3"><%= data.kernel %></td>
          </tr>
          <tr>
            <td class="col-xs-3">Hostname</td>
            <td class="col-xs-3"><%= data.hostname %></td>
          </tr>
          <tr>
            <td class="col-xs-3">IP</td>
            <td class="col-xs-3"><%= data.ip %></td>
          </tr>
        </tbody>
      </table>
      <!-- bottom -->
      <!--  -->
    </div>
  </div>
</div>
  <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
<script src='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js'></script>

    <script src="javascripts/index.js"></script>

</body>
</html>
